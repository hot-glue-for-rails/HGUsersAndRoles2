<%= turbo_frame_tag "__#{dom_id(user)}" do  %>
<div class="cell editable" style="position: relative;">
  <% if user.errors.any? %>
  <%= render(partial: "errors", locals: {resource: user }) %>
  <% end %>
  <h2>Editing <%= user.name %></h2>
  <%= form_with model: user, url: user_path(user) do |f| %>
  <%=  render partial: "users/form",  locals: {:user => user, f: f} %>
  <% end %>
</div>
<% end %>
